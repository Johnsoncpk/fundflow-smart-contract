	+ðŸ”balanceOf()
	+ðŸ”ownerOf()
	+ðŸ”name()
	+ðŸ”symbol()
	+ðŸ”tokenURI()
	#ðŸ”_baseURI()
	+approve()
	+ðŸ”getApproved()
	+setApprovalForAll()
	+ðŸ”isApprovedForAll()
	+transferFrom()
	+safeTransferFrom()
	+safeTransferFrom()
