	{abstract}ERC721

    ' -- usingFor --
	{abstract}ðŸ“šStrings for [[uint256]]

    ' -- vars --
	-[[mapping uint256=>string ]] _tokenURIs

    ' -- methods --
	+ðŸ”tokenURI()
	#_setTokenURI()
